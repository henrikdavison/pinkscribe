tic><characteristic name="S" typeId="59b1-319e-ec13-d466">4</characteristic><characteristic name="AP" typeId="75aa-a838-b675-6484">-2</characteristic><characteristic name="D" typeId="ae8a-3137-d65b-4ca7">1</characteristic><characteristic name="Abilities" typeId="837d-5e63-aeb7-1410">-</characteristic></characteristics></profile></profiles></selection></selections><costs><cost name="pts" typeId="points" value="40" /><cost name=" Cabal Points" typeId="6a06-928d-a42e-4293" value="1" /></costs></selection><selection id="r4oawk7alyjolhr27lbt0r" name="Terminator" entryId="742d-777b-2b31-5e71::0f71-fbb2-b6d6-a5b5" entryGroupId="742d-777b-2b31-5e71::b7bd-5c16-d4ee-699e" number="3" type="model"><selections><selection id="1a4ajz9oqirrobvjzz7enb" name="Inferno combi-bolter" entryId="742d-777b-2b31-5e71::d778-32a6-2e2f-b74e::b493-4d6f-efcc-a068" entryGroupId="" number="3" type="upgrade"><profiles><profile id="c5ec-eb14-b836-ca40" name="Inferno combi-bolter" hidden="false" typeId="d5f97c0b-9fc9-478d-aa34-a7c414d3ea48" typeName="Weapon"><characteristics><characteristic name="Range" typeId="6fa97fa8-ea74-4a27-a0fb-bc4e5f367464">24&amp;quot;</characteristic><characteristic name="Type" typeId="077c342f-d7b9-45c6-b8af-88e97cafd3a2">Rapid Fire 2</characteristic><characteristic name="S" typeId="59b1-319e-ec13-d466">4</characteristic><characteristic name="AP" typeId="75aa-a838-b675-6484">-2</characteristic><characteristic name="D" typeId="ae8a-3137-d65b-4ca7">1</characteristic><characteristic name="Abilities" typeId="837d-5e63-aeb7-1410">-</characteristic></characteristics></profile></profiles></selection><selection id="ia48k6bgs6d0106nes4bl" name="Prosperine khopesh" entryId="742d-777b-2b31-5e71::bd59-9d46-135f-3b9f" entryGroupId="" number="3" type="upgrade"><profiles><profile id="4fb0-94b7-1309-cf59" name="Prosperine khopesh" hidden="false" typeId="d5f97c0b-9fc9-478d-aa34-a7c414d3ea48" typeName="Weapon"><characteristics><characteristic name="Range" typeId="6fa97fa8-ea74-4a27-a0fb-bc4e