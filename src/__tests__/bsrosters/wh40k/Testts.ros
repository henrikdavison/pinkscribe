<roster id="638f517350e1e4fc6e56d6a5" name="Testts" battleScribeVersion="2.03" gameSystemId="28ec-711c-d87f-3aeb" gameSystemName="Warhammer 40,000 9th Edition" gameSystemRevision="225" xmlns="http://www.battlescribe.net/schema/rosterSchema"><costs><cost name=" PL" typeId="e356-c769-5920-6e14" value="106" /><cost name="CP" typeId="2d3b-b544-ad49-fb75" value="2" /><cost name="pts" typeId="points" value="1997" /></costs><costLimits><costLimit name="pts" typeId="points" value="2000" /></costLimits><forces><force id="638f517350e1e4fc6e56d6c2" name="Battalion Detachment 0CP" entryId="564e-55d5-79bc-a4d7" catalogueId="ebe8-544e-1fe8-fcde" catalogueRevision="195" catalogueName="Imperium - Adeptus Mechanicus"><selections><selection id="638f517350e1e4fc6e56d7a9" name="Army of Renown - Skitarii Veteran Cohort" entryId="7967-3985-9e3c-e265::ac03-d781-0c3b-b4be" number="1" type="upgrade"><categories><category id="fcff-0f21-93e6-1ddc" entryId="fcff-0f21-93e6-1ddc" name="Configuration" primary="true" /></categories></selection><selection id="638f517350e1e4fc6e56d7aa" name="Battle Size" entryId="44e3-c224-ba82-1b55::f29d-8a5d-18b6-a071" number="1" type="upgrade"><selections><selection id="638f517350e1e4fc6e56d7b0" name="3. Strike Force (101-200 Total PL / 1001-2000 Points) " entryId="44e3-c224-ba82-1b55::09f9-b586-8d63-7635" entryGroupId="44e3-c224-ba82-1b55::2883-57e8-87a6-38e2" number="1" type="upgrade"><costs><cost name="CP" typeId="2d3b-b544-ad49-fb75" value="6" /></costs></selection></selections><costs><cost name="CP" typeId="2d3b-b544-ad49-fb75" value="6" /></costs><categories><category id="fcff-0f21-93e6-1ddc" entryId="fcff-0f21-93e6-1ddc" name="Configuration" primary="true" /></categories></selection><selection id="638f517350e1e4fc6e56d7bb" name="Detachment Command Cost" entryId="ec87-f19e-eee2-1ba8::9d97-2793-9882-d48a" number="1" type="upgrade"><categories><category id="fcff-0f21-93e6-1ddc" entryId="fcff-0f21-93e6-1ddc" name="Configuration" primary="true" /></categories></selection><selection id="638f517350e1e4fc6e56d7be" name="Forge World Choice" entryId="4fdc-b9a7-3fe7-9e84::0dbb-e7cd-4b8e-92aa" number="1" type="upgrade"><selections><selection id="638f517350e1e4fc6e56d7cc" name="Forge World: Mars" entryId="4fdc-b9a7-3fe7-9e84::1202-78d3-2c86-8c31::a629-aac7-aa2d-ddfd::bde4-e5f7-024d-78a4" entryGroupId="4fdc-b9a7-3fe7-9e84::1202-78d3-2c86-8c31::8f3b-b73c-bc99-e19a" number="1" type="upgrade"><rules><rule id="77df-46cb-176f-8bf9" name="Dogma - Masters of the Forge" hidden="false"><description>
