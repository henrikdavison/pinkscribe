                    <characteristic name="Description" typeId="21befb24-fc85-4f52-a745-64b2e48f8228">This model has a 4+ invulnerable save.</characteristic>
                  </characteristics>
                </profile>
                <profile id="628e-8578-f0b2-0132::af59-6add-785b-4bed::7772-859e-7414-ae8e::2d1c-58ec-e7e5-5c69" name="Warlock Conclave" publicationId="1f62-db93-63f4-4dc7" page="158" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">
